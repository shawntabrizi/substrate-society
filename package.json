{
  "name": "substrate-society",
  "version": "0.1.1",
  "private": true,
  "author": "Shawn Tabrizi <shawntabrizi@gmail.com>",
  "license": "Unlicense",
  "dependencies": {
    "@polkadot/api": "^3.10.2",
    "@polkadot/extension-dapp": "^0.37.1",
    "@polkadot/keyring": "^5.7.1",
    "@polkadot/react-identicon": "^0.69.1",
    "@polkadot/ui-keyring": "^0.69.1",
    "@polkadot/util": "^5.7.1",
    "@polkadot/util-crypto": "^5.7.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.0",
    "toformat": "^2.0.0"
  },
  "devDependencies": {
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.1.1",
    "react-app-rewired": "^2.1.8"
  },
  "scripts": {
    "start": "PORT=8000 react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "CI=true react-scripts test --env=jsdom",
    "eject": "react-app-rewired eject",
    "lint": "eslint src/**",
    "lint:ci": "eslint src/** --max-warnings=0",
    "lint:fix": "eslint --fix src/**",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "standard"
    ],
    "plugins": [
      "only-warn"
    ],
    "rules": {
      "semi": [
        1,
        "always"
      ],
      "no-extra-semi": 1
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://shawntabrizi.github.io/substrate-society",
  "bugs": {
    "url": "https://github.com/shawntabrizi/substrate-society/issues"
  },
  "keywords": [
    "substrate",
    "substrate-ui",
    "polkadot-js",
    "society"
  ]
}
